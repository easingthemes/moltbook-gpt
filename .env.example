OPENAI_API_KEY=sk-proj-your-key-here
# Optional: model for LLM (default gpt-4o-mini). Use gpt-5.1-instant to test GPT-5.1 Instant.
# OPENAI_MODEL=gpt-5.1-instant
MOLTBOOK_API_KEY=moltbook_sk_YOUR-KEY-HERE
MOLTBOOK_API_URL=https://www.moltbook.com/api/v1
MOLTBOOK_USE_MOCKS=false
AGENT_NAME=Ejaj
# How often we run a tick (feed check, AI decision). Default 5 min.
# TICK_INTERVAL_MINUTES=5
# Min minutes between posts to Moltbook (rate limit). Default 30, min 30.
POST_INTERVAL_MINUTES=30
DRY_RUN=false
KILL_SWITCH=false
# Optional: first post when agent has never posted (only if DRY_RUN=false)
# FIRST_POST_SUBMOLT=general
# FIRST_POST_TITLE=Hello from Ejaj
# FIRST_POST_CONTENT=Hi, I'm Ejaj â€” just joined Moltbook. ðŸ¦ž
# USE_PERSONALIZED_FEED=false
# MOLTBOOK_SKILL_PATH=./moltbook/SKILL.md
# Submolts to subscribe (npm run submolt:subscribe). Comma-separated. Default: general
# SUBSCRIBE_SUBMOLTS=general,tech
# Avatar upload (npm run avatar:upload): default path when no CLI arg
# AVATAR_PATH=data/ejaj.gif
